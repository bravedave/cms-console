{
    "name": "bravedave/cms-console",
    "description": "CMS Console Access",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Bray",
            "email": "david@brayworth.com.au"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/bravedave/dvc"
        }
    ],
    "require": {
        "ext-sqlsrv": "*",
        "bravedave/dvc": "*"
    },
    "autoload": {
        "psr-4": {
            "cms\\console\\": "src/console/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/dev/app/"
        }
    },
    "scripts": {
        "post-update-cmd": "cms\\console\\postUpdate::upgrade"
    }

}